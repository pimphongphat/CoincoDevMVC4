@using Telerik.Web.Mvc.UI
@model Coinco.SMS.Website.Models.ServiceOrderLine
  @Html.Label("Part Number:", new { @class = "labelHeaderSPView", id = "labelPartNumber" })

    @if (Model.PartNumber != null)
    {
        @Html.Label(Model.PartNumber, new { id = "lblPartNumber" })
    }
    else
    {
        @Html.Label("", new { id = "lblPartNumber" })
    }